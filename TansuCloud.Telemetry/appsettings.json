{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OpenTelemetry": {
    "Otlp": {
      "Enabled": false,
      "Endpoint": "",
      "Comments": "OTLP export is disabled in production for the Telemetry service to avoid circular dependencies and ensure self-contained operation. The service continues to function normally without SigNoz connectivity."
    }
  },
  "Telemetry": {
    "Ingestion": {
      "ApiKey": "replace-with-secure-key-please-change",
      "QueueCapacity": 4096,
      "EnqueueTimeout": "00:00:05"
    },
    "Admin": {
      "ApiKey": "replace-with-admin-key",
      "DefaultPageSize": 50,
      "MaxPageSize": 200,
      "MaxExportItems": 500
    },
    "Database": {
      "FilePath": "App_Data/telemetry/telemetry.db",
      "EnforceForeignKeys": true
    }
  },
  "AllowedHosts": "*"
}
