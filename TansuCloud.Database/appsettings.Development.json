{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OpenTelemetry": {
    "Otlp": {
  "Endpoint": "http://127.0.0.1:4317"
    }
  },
  "Outbox": {
    "PollSeconds": 2,
    "BatchSize": 100,
    "MaxAttempts": 8,
    "RedisConnection": "",
    "Channel": "tansu.outbox"
  },
  "Provisioning": {
    "AdminConnectionString": "Host=localhost;Port=5432;Database=postgres;Username=postgres;Password=postgres",
    "RuntimeConnectionString": "Host=localhost;Port=6432;Database=postgres;Username=postgres;Password=postgres",
    "DatabaseNamePrefix": "tansu_tenant_",
    "Extensions": "citus,vector"
  },
  "Dev": {
    "ProvisionBypassKey": "letmein"
  },
    "Audit": {
      "ConnectionString": "Host=localhost;Port=5432;Database=tansu_audit;Username=postgres;Password=postgres",
      "Table": "audit_events",
      "ChannelCapacity": 20000,
      "FullDropEnabled": true,
      "MaxDetailsBytes": 16384
    }
}
