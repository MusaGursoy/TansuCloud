{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "GatewayBaseUrl": "http://127.0.0.1:8080",
  "Oidc": {
  "Authority": "http://127.0.0.1:8080/identity",
  "MetadataAddress": "http://127.0.0.1:8080/identity/.well-known/openid-configuration",
  "AcceptAnyServerCert": true,
  "ClientId": "tansu-dashboard",
    "ClientSecret": "dev-secret"
  },
  "OpenTelemetry": {
    "Otlp": {
  "Endpoint": "http://127.0.0.1:4317"
    }
  },
  "LoggingReport": {
    "Enabled": false,
    "MinimumLevel": "Warning",
    "MaxBufferEntries": 1000,
    "ReportInterval": "00:30:00"
  },
  "Audit": {
    "ConnectionString": "Host=localhost;Port=5432;Database=tansu_audit;Username=postgres;Password=postgres",
    "Table": "audit_events",
    "ChannelCapacity": 20000,
    "FullDropEnabled": true,
    "MaxDetailsBytes": 16384
  }
}
