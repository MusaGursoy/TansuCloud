{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "SigNozQuery": {
    "ApiBaseUrl": "http://signoz:3301",
    "ApiKey": "",
    "TimeoutSeconds": 60,
    "MaxRetries": 3,
    "EnableQueryAllowlist": true,
    "SigNozUiBaseUrl": "https://signoz.example.com"
  },
  "Oidc": {
    "AcceptAnyServerCert": false,
    "ClientId": "tansu-dashboard",
    "ClientSecret": ""
  },
  "OpenTelemetry": {
    "Otlp": {
      "Endpoint": "http://signoz-otel-collector:4317",
      "Diagnostics": {
        "EnableGrpcActivityListener": false
      }
    }
  },
  "LoggingReport": {
    "Enabled": true,
    "MinimumLevel": "Error",
    "MaxBufferEntries": 5000,
    "ReportInterval": "00:15:00"
  },
  "LogReporting": {
    "Enabled": true,
    "MainServerUrl": "",
    "ApiKey": "",
    "ReportIntervalMinutes": 30,
    "SeverityThreshold": "Error",
    "QueryWindowMinutes": 30,
    "MaxItems": 1000,
    "HttpTimeoutSeconds": 60,
    "WarningSamplingPercent": 5,
    "AllowedWarningCategories": [
      "OIDC-",
      "Tansu.Gateway",
      "Tansu.Storage",
      "Tansu.Database",
      "Tansu.Identity"
    ],
    "HttpLatencyP95Ms": 1000,
    "DbDurationP95Ms": 500,
    "ErrorRatePercent": 0.5,
    "PseudonymizeTenants": true,
    "TenantHashSecret": ""
  },
  "Audit": {
    "ConnectionString": "",
    "Table": "audit_events",
    "ChannelCapacity": 50000,
    "FullDropEnabled": false,
    "MaxDetailsBytes": 32768
  }
}
