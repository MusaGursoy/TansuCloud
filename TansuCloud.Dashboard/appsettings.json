{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenTelemetry": {
    "Otlp": {
      "Endpoint": "http://localhost:4317"
    }
  },
  "Oidc": {
    "ClientId": "tansu-dashboard",
    "ClientSecret": "dev-secret",
    "AcceptAnyServerCert": true,
    "PreloadDiscovery": true
  },
  "LogReporting": {
    "Enabled": true,
    "MainServerUrl": "",
    "ReportIntervalMinutes": 60,
    "SeverityThreshold": "Warning",
    "QueryWindowMinutes": 60,
    "MaxItems": 2000,
    "HttpTimeoutSeconds": 30,
    "WarningSamplingPercent": 10,
    "AllowedWarningCategories": [
      "OIDC-",
      "Tansu.Gateway",
      "Tansu.Storage",
      "Tansu.Database",
      "Tansu.Identity"
    ],
    "HttpLatencyP95Ms": 500,
    "DbDurationP95Ms": 300,
    "ErrorRatePercent": 1,
    "PseudonymizeTenants": true,
    "TenantHashSecret": ""
  }
}
